{
  "name": "andersundsehr/github-php-compatibility-checker",
  "description": "Checks your Organisation for PHP compatibility",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Matthias Vogel",
      "email": "m.vogel@andersundsehr.com"
    }
  ],
  "require": {
    "php": "~8.5.0",
    "composer/semver": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "pluswerk/grumphp-config": "^10.2"
  },
  "autoload": {
    "psr-4": {
      "Andersundsehr\\GithubPhpCompatibilityChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Andersundsehr\\GithubPhpCompatibilityChecker\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true
    }
  }
}
